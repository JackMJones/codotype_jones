    "bluebird": "^3.5.1",
    "ejs": "^2.6.1",
    "fs-extra": "^7.0.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "chai": "^4.1.2",
    "jest": "^24.9.0",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1",
    "ts-jest": "^24.2.0",
    "typescript": "~3.4.5"
  },
  "bugs": {
    "url": "https://github.com/codotype/codotype/issues"
